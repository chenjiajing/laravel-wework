{
    "name": "chenjiajing/laravel-wework",
    "description": "laravel-wework is laravel package for can be fast develop wework",
    "license": "MIT",
    "authors": [
        {
            "name": "chenjiajing",
            "email": "i@chenjiajing.com"
        }
    ],
    "require": {
      "ext-json": "*",
      "ext-curl": "*",
      "ext-libxml": "*",
      "ext-openssl": "*",
      "ext-mbstring": "*",
      "ext-simplexml": "*"
    },
    "autoload": {
        "classmap": [
